program teste6
declare
  integer a, b, c, maior;
begin
  read(a);
  read(b);
  read(c);

  % identifica o maior
  maior:= (a>b and a>c)?a:(b>c)?b:c;
  a := "Teste de literal";

  if ( mod(b,2) <> 0 ) then
    b := true;
  end

  if (b) then
    c := 3;
    c := c**2;
    c /= 1;
    write ( ones(2 >> c) )
  end

  write (b >= 3);

